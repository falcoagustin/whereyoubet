<div class="teams-container">
    <% @teams.each do |team| %>
      <div>
        <label> <%= team.name %> </label>
        <label> <%= team.country_id %> </label>
        <!-- <%= link_to 'Edit', edit_team_path(team) %> -->
        <%= link_to 'Destroy', team_path(team),
                method: :delete,
                data: { confirm: 'Are you sure?' } %>
      </div>
    <% end %>
</div>
